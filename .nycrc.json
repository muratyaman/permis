{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/__tests__/**/*.ts"
  ],
  "statements": 50,
  "branches": 50,
  "functions": 50,
  "lines": 50,
  "reporter": [
    "lcov",
    "text",
    "text-summary",
    "html"
  ]
}